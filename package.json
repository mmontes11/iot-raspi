{
  "name": "iot_raspi",
  "version": "1.0.0",
  "description": "IoT container for Raspberry Pi that uses IoT client",
  "license": "UNLICENSED",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node src/index",
    "dist-client": "cd src/lib/iot_client && npm run dist && cd - && cp -R src/lib/iot_client/dist/ dist/lib/",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "build": "babel -d dist/ src/",
    "dist": "npm run build && cp package.json dist/ && cd dist && npm install --production && cd - && npm run dist-client",
    "production": "cross-env NODE_ENV=production node dist/index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmontes11/iot_raspi"
  },
  "keywords": [
    "js",
    "javacript",
    "es6",
    "nodejs",
    "raspberrypi",
    "iot"
  ],
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "bugs": {
    "url": "https://github.com/mmontes11/iot_raspi/issues"
  },
  "homepage": "https://github.com/mmontes11/iot_raspi#readme",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "cross-env": "^5.1.3",
    "iot_client": "file:./src/lib/iot_client",
    "node-dht-sensor": "0.0.32",
    "onoff": "^1.1.7",
    "public-ip": "^2.3.5",
    "underscore": "^1.8.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.0"
  }
}
