{
  "name": "iot-raspi-sensors",
  "version": "1.0.1",
  "description": "IoT container for Raspberry Pi used to measure temperature and humidity periodically.",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node src/index",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "build": "babel -d dist/ src/",
    "dist": "npm run build && cp -R package.json scripts/ dist/ && cd dist && npm install --production && cd -"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mmontes11/iot-raspi"
  },
  "keywords": [
    "js",
    "javacript",
    "es6",
    "nodejs",
    "docker",
    "iot",
    "raspberrypi",
    "raspi",
    "sensors",
    "dht11",
    "dht22",
    "ds18b20"
  ],
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "bugs": {
    "url": "https://github.com/mmontes11/iot-raspi/issues"
  },
  "homepage": "https://github.com/mmontes11/iot-raspi#readme",
  "dependencies": {
    "@mmontes11/iot-client": "^0.0.1",
    "bluebird": "^3.5.1",
    "cross-env": "^5.1.3",
    "ds18b20": "^0.1.0",
    "node-dht-sensor": "0.0.34",
    "onoff": "^1.1.7",
    "public-ip": "^2.3.5",
    "underscore": "^1.8.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0"
  }
}
